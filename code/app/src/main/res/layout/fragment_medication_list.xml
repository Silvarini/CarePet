<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/_315sdp"
    android:layout_height="@dimen/_562sdp"
    android:id="@+id/medicationListFragment"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp"
    android:fillViewport="true"
    android:background="@color/cardview_light_background"
    tools:context=".medication.MedicationListFragment">


    <Button
        android:id="@+id/button_new_medication"
        android:layout_width="260dp"
        android:layout_height="58dp"
        android:layout_marginTop="52dp"
        android:text="Adicionar Medication"
        app:layout_constraintBottom_toTopOf="@+id/rv_medicationList"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_medicationList"
        android:layout_width="366dp"
        android:layout_height="531dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
